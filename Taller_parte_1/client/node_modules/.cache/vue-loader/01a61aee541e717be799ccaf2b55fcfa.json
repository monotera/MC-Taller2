       v-model=\"nameText\"\n              :counter=\"20\"\n              :rules=\"nameRules\"\n              label=\"Nombre\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"salida\"\n              :counter=\"40\"\n              :rules=\"lugarRules\"\n              label=\"Lugar de salida\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"llegada\"\n              :counter=\"40\"\n              :rules=\"lugarRules\"\n              label=\"Lugar de llegada\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"fecha\"\n              :counter=\"10\"\n              :rules=\"dateRules\"\n              label=\"Fecha (aaaa-mm-dd)\"\n              required\n            ></v-text-field>\n            <v-btn\n              :disabled=\"!valid\"\n              color=\"success\"\n              class=\"mr-4\"\n              @click=\"validate\"\n            >\n              Crear paseo\n            </v-btn>\n          </v-form>\n        </v-col>\n      </v-row>\n    </v-container>\n  </v-main>\n</template>\n\n<script>\nimport axios from \"axios\";\nexport default {\n  data: () => ({\n    valid: true,\n    nameText: \"\",\n    salida: \"\",\n    llegada: \"\",\n    fecha: \"\",\n    nameRules: [\n      (v) => !!v || \"Se requiere el nombre\",\n      (v) =>\n        (v && v.length <= 20) || \"El nombre no debe exceder los 20 caracteres\",\n    ],\n    dateRules: [\n      (v) => !!v || \"Se requiere la fecha\",\n      (v) =>\n        (v && v.length <= 10) || \"El nombre no debe exceder los 10 caracteres\",\n    ],\n    lugarRules: [\n      (v) => !!v || \"Se requiere el lugar\",\n      (v) =>\n        (v && v.length <= 40) || \"El nombre no debe exceder los 40 caracteres\",\n    ],\n    select: null,\n  }),\n\n  methods: {\n    validate() {\n      if (this.$refs.form.validate()) {\n        let url = `${this.$store.getters.getUrlBase}create_trip`;\n        axios\n          .post(url, {\n            name: this.nameText,\n            place_departure: this.salida,\n            place_arrival: this.llegada,\n            date: this.fecha,\n          })\n          .then(() => this.$router.push(\"/\"));\n      }\n    },\n  },\n};\n</script>\n\n<style scoped>\nh1 {\n  text-align: center;\n  width: 100%;\n  margin-top: 5rem;\n}\n</style>\n"]}]}